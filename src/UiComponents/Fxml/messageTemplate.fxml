<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<Label fx:id="container" alignment="CENTER" maxWidth="360.0" minWidth="360.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UiComponents.Controllers.MessageTemplateController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <graphic>
      <VBox fx:id="containerMsg" maxWidth="280.0" spacing="5.0" style="-fx-border-radius: 100px; -fx-background-radius: 100px;">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <TextFlow fx:id="textContent" maxWidth="280.0" />
         </children>
      </VBox>
   </graphic>
</Label>
